{
  "name": "openclassrooms/doctrine-cache-extension",
  "description": "Library extending doctrine cache capabilities ",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Romain Kuzniak",
      "email": "romain.kuzniak@openclassrooms.com"
    },
    {
      "name": "OpenClassrooms",
      "email": "tech@openclassrooms.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenClassrooms\\": "src/OpenClassrooms/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenClassrooms\\Tests\\": "tests/OpenClassrooms/"
    }
  },
  "require": {
    "php": ">=5.6",
    "doctrine/cache": "~1.0"
  },
  "require-dev": {
    "mockery/mockery": "dev-master",
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
